package wrq;

public class BubbleSort {                              
	public static void main(String[] args) {
		int a[]= {9,2,1,3,6,5};
		BubbleSort asort=new BubbleSort();
		asort.sort(a);
	}
	public void sort(int[] a) {
		for(int i=1;i<a.length;i++) {
			for(int j=0;j<a.length-1;j++) {
				if(a[j]>a[j+1]){
					int temp=a[j];
					a[j]=a[j+1];
					a[j+1]=temp;
				}
			}	
		}
		showArray(a);
	}
	public void showArray(int[] a) {
		for(int i:a) {
			System.out.print(i);
	}
		
	}

}
